{
    "name": "tijmenwierenga/snowplow-tracker",
    "description": "Snowplow tracker for PHP powered applications",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ramsey/uuid": "^4.2.3",
        "ramsey/collection": "^1.2.0",
        "psr/http-client": "^1.0",
        "php-http/discovery": "^1.14",
        "php-http/httplug": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.5",
        "vimeo/psalm": "^4.13 || ^5.0",
        "friendsofphp/php-cs-fixer": "^3.2",
        "nyholm/psr7": "^1.4",
        "psalm/plugin-phpunit": "^0.16.1 || ^0.17.0 || ^0.18.0",
        "rector/rector": "^0.12.4 || ^0.13.0 || ^0.14.0",
        "symfony/http-client": "^5.3 || ^6.0",
        "symfony/var-dumper": "^5.3 || ^6.0",
        "infection/infection": "^0.25.3 || ^0.26.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TijmenWierenga\\SnowplowTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TijmenWierenga\\Tests\\SnowplowTrackers\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tijmen Wierenga",
            "email": "t.wierenga@live.nl"
        }
    ],
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
