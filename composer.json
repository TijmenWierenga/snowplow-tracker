{
    "name": "tijmenwierenga/snowplow-tracker",
    "description": "Snowplow tracker for PHP powered applications",
    "type": "library",
    "require": {
        "php": "^8.1",
        "ramsey/uuid": "^4.2",
        "symfony/http-client": "^5.3",
        "phpunit/phpunit": "^9.5"
    },
    "require-dev": {
        "vimeo/psalm": "^4.10",
        "friendsofphp/php-cs-fixer": "^3.2",
        "psalm/plugin-phpunit": "^0.16.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TijmenWierenga\\SnowplowTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TijmenWierenga\\Tests\\SnowplowTrackers\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tijmen Wierenga",
            "email": "t.wierenga@live.nl"
        }
    ]
}
